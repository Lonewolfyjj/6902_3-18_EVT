CONFIG_RT_USING_RESET=y
# CONFIG_RT_USING_CRU_DUMP is not set
# CONFIG_RT_USING_REGULATOR_DUMP is not set
# CONFIG_RT_USING_PROF_IRQ is not set
# CONFIG_RT_USING_PMIC is not set
# CONFIG_RT_USING_PWM_REGULATOR is not set
CONFIG_RT_USING_CACHE=y
CONFIG_RT_USING_UNCACHE_HEAP=y
CONFIG_RT_UNCACHE_HEAP_ORDER=0x0E
CONFIG_RT_USING_LARGE_HEAP=y
CONFIG_RT_LARGE_MALLOC_THRRESH=512
CONFIG_RT_LARGE_HEAP_SIZE=524288
# CONFIG_RT_USING_DTCM_HEAP is not set
# CONFIG_RT_USING_PSRAM_HEAP is not set

#
# Enable Fault Dump Hook
#
# CONFIG_RT_USNING_FAULT_DUMP_HOOK is not set
CONFIG_RT_USING_SNOR=y
CONFIG_RT_SNOR_SPEED=80000000
CONFIG_RT_SNOR_DUAL_IO=y
CONFIG_RT_USING_SNOR_FSPI_HOST=y
# CONFIG_RT_USING_SNOR_SFC_HOST is not set
# CONFIG_RT_USING_SNOR_SPI_HOST is not set

#
# RT-Thread rockchip jpeg enc driver
#
# CONFIG_RT_USING_JPEG_ENC is not set

#
# RT-Thread rockchip pm drivers
#
CONFIG_RT_USING_PM_DVFS=y
CONFIG_RT_USING_PM_REQ_PWR=y
# CONFIG_RT_USING_PM_REQ_CLK is not set
# CONFIG_RT_USING_DVFS_DBG is not set
# CONFIG_RT_USING_PM_RUNTIME is not set
# CONFIG_RT_USING_PM_TIMER is not set

#
# RT-Thread rockchip mipi-dphy driver
#
# CONFIG_RT_USING_MIPI_DPHY is not set

#
# RT-Thread rockchip isp driver
#
# CONFIG_RT_USING_ISP is not set

#
# RT-Thread rockchip vcm driver
#
# CONFIG_RT_USING_VCM is not set

#
# RT-Thread rockchip vicap driver
#
# CONFIG_RT_USING_VICAP is not set

#
# RT-Thread rockchip camera driver
#
# CONFIG_RT_USING_CAMERA is not set

#
# RT-Thread rockchip vicap_lite driver
#
# CONFIG_RT_USING_VICAP_LITE is not set

#
# RT-Thread rockchip csi2host driver
#
# CONFIG_RT_USING_CSI2HOST is not set

#
# RT-Thread rockchip buffer_manage driver
#
# CONFIG_RT_USING_BUFFER_MANAGE is not set
# CONFIG_RT_USING_SENSOR_HUB is not set
# CONFIG_RT_USING_QPIPSRAM is not set

#
# RT-Thread rockchip coredump driver
#
# CONFIG_RT_USING_COREDUMP is not set
# CONFIG_RT_USING_SPINAND is not set

#
# Enable PSTORE
#
# CONFIG_RT_USING_PSTORE is not set

#
# RT-Thread rockchip rk2108 drivers
#

#
# Enable Audio
#
CONFIG_RT_USING_CODEC=y
CONFIG_RT_USING_CODEC_ES8311=y
# CONFIG_RT_USING_DRIVER_AUDIO_PCM_PLUGIN is not set

#
# Audio Cards
#
CONFIG_RT_USING_AUDIO_CARD=y
# CONFIG_RT_USING_AUDIO_CARD_ACDCDIG is not set
# CONFIG_RT_USING_AUDIO_CARD_AUDIOPWM is not set
# CONFIG_RT_USING_AUDIO_CARD_I2S_MIC is not set
CONFIG_RT_USING_AUDIO_CARD_PDM_MIC=y
# CONFIG_RT_USING_AUDIO_CARD_PCM_BT is not set
# CONFIG_RT_USING_AUDIO_CARD_AW8896 is not set
# CONFIG_RT_USING_AUDIO_CARD_ES7243 is not set
# CONFIG_RT_USING_AUDIO_CARD_ES8156 is not set
CONFIG_RT_USING_AUDIO_CARD_ES8311=y
# CONFIG_RT_USING_AUDIO_CARD_ES8388 is not set
# CONFIG_RT_USING_AUDIO_CARD_ADC_ES8311_ECHO is not set
# CONFIG_RT_USING_AUDIO_CARD_PDM_ES8388_ECHO is not set

#
# Audio Interfaces
#
CONFIG_RT_USING_PCM=y
CONFIG_RT_USING_I2STDM=y
CONFIG_RT_USING_I2STDM1=y
CONFIG_RT_USING_PDM=y
CONFIG_RT_USING_PDM0=y
# CONFIG_RT_USING_VAD is not set
CONFIG_RT_USING_CRU=y
CONFIG_RT_USING_DMA=y
CONFIG_RT_USING_DMA_PL330=y
CONFIG_RT_USING_PMU=y
# CONFIG_RT_USING_DISPLAY is not set
# CONFIG_RT_USING_KEYCTRL is not set
CONFIG_RT_USING_PWM0=y

#
# Enable DSP
#
CONFIG_RT_USING_DSP=y
CONFIG_RT_USING_DSPFW_LOADER=y
CONFIG_RT_USING_DSPFW_BUILTIN=y
# CONFIG_RT_USING_DSPFW_FILE is not set
CONFIG_RT_DSPFW_FILE_NAME="rkdsp_fw.c"
# CONFIG_RT_USING_DSP_TRACE is not set
CONFIG_RT_DSP_UART_PORT=0

#
# Enable UART
#
CONFIG_RT_USING_UART=y
CONFIG_RT_USING_UART0=y
CONFIG_RT_USING_UART1=y
CONFIG_RT_USING_UART2=y

#
# Enable I2C
#
CONFIG_RT_USING_I2C0=y
CONFIG_RT_USING_I2C1=y
CONFIG_RT_USING_I2C2=y

#
# Enable SPI
#
CONFIG_RT_USING_SPI2APB=y
CONFIG_RT_USING_SPI1=y
CONFIG_RT_USING_SPI2=y
