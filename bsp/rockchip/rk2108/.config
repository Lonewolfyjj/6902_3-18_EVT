#
# RT-Thread application
#
# CONFIG_RT_USING_LVGL_APP_DEMO is not set
# CONFIG_RT_USING_ROCKCHIP_DEMO is not set
CONFIG_RT_USING_RK_IOT_APP=y
CONFIG_PRJCONF_SYSINFO_SAVE_TO_FLASH=y
CONFIG_PRJCONF_BOOT_AUTOMATICALLY=y
# CONFIG_PRJCONF_NET_EN is not set
# CONFIG_PRJCONF_USING_AISPEECH is not set
CONFIG_PRJCONF_AI_RK=y
# CONFIG_PRJCONF_AI_SPEECH is not set
# CONFIG_PRJCONF_AI_CUSTOM is not set
# CONFIG_PRJCONF_AI_LWAKEUP is not set
CONFIG_PRJCONF_PLAYBACK_SOUND_CARD="es8311p"
CONFIG_PRJCONF_ASR_SOUND_CARD="pdmc"
# CONFIG_PRJCONF_ASR_USE_VAD is not set
# CONFIG_ENABLE_DUAL_TRACKING is not set

#
# RT-Thread bsp test case
#
# CONFIG_RT_USING_PRIVATE_TEST is not set

#
# RT-Thread Common Test case
#
# CONFIG_RT_USING_TC is not set
# CONFIG_RT_USING_FILE_TEST is not set
# CONFIG_RT_USING_COMMON_TEST is not set
CONFIG_RT_USING_FWANALYSIS=y
